{
  "expo": {
    "name": "Tracing Paper Sketching",
    "slug": "tracing-paper-sketching",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.advancedweb.tracing_paper_sketching",
      "permissions": [ "CAMERA", "READ_EXTERNAL_STORAGE" ],
      "versionCode": 14
    }
  }
}
